    "Lt 1";
    "Lt 2" [fillcolor=red,style=filled];
    Gen -> "Lt 1" [label="A"];
    Gen -> "Lt 2" [label="A"];
    "Lt 2" -> "Lt 1" [constraint=false;label="R"];
}
digraph D { // byzantine.dot.4.svg
    Gen [fillcolor=red,style=filled];
    "Lt 1";
    "Lt 2" [fillcolor=red,style=filled];
